<?xml version="1.0" encoding="UTF-8"?>
<bot>
	<token>xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx</token>
	<prefix>!</prefix>
	<commands>
	
		<command>
			<title>help</title>
			<syntax>help &lt;optional: command&gt;</syntax>
			<description>
			Will DM the user a list of commands, or will display help about the specified command.
			</description>
		</command>
		
		<command>
			<title>disconnect</title>
			<syntax>disconnect</syntax>
			<description>
			Disconnects the bot from Discord.
			</description>
		</command>
	
	</commands>
</bot>
